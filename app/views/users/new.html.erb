<br><br>
<div align="center"><h1>Create a Profile</h1>
<br>
<%= form_for @user do |f| %>
Name: <%= f.text_field :name %>
Age: <%= f.number_field :age %>
Password: <%= f.password_field :password %>
<%=f.submit "Create Account"%>
<%end%>
<br>
<br>

<% if flash[:messages] %>
<%= flash.now[:messages] %>
<%end%>


</div>